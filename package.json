    "clsx": "^2.0.0",
    "connectkit": "1.5.3",
    "copy-to-clipboard": "3.3.1",
    "date-fns": "2.29.3",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "5.0.0",
    "ethers": "5.7.2",
    "framer-motion": "8.5.0",
    "jotai": "2.4.1",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "next": "13.5.1",
